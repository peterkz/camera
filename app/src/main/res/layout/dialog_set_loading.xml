<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/video_loading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="16dp"
        android:layout_centerHorizontal="true"
        android:visibility="visible"
        android:gravity="center_vertical" >

        <TextView
            android:paddingTop="16dp"
            android:id="@+id/video_loading_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="正在设置联网："
            android:textSize="20.0sp" />

        <!--<lib.wetoop.galaeye.dialog.NumberProgressBar
            android:id="@+id/galaeye_numberbar1"
            android:layout_below="@+id/video_loading_text"
            android:layout_width="wrap_content"
            android:padding="20dp"
            custom:galaeye_progress_current="0"
            style="@style/galaeye_NumberProgressBar_Default"
            android:layout_height="wrap_content" />-->
        <ProgressBar
            android:id="@+id/probar"
            android:layout_below="@+id/video_loading_text"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="300dp"
            android:layout_height="16dp"
            android:max="100"
            android:progress="0"/>
    </RelativeLayout>

</RelativeLayout>